{
  "name": "yup-locale-ja",
  "version": "0.1.3",
  "license": "MIT",
  "contributors": [
    {
      "name": "Shota Fuji",
      "email": "pockawoooh@gmail.com",
      "url": "https://github.com/pocka"
    }
  ],
  "repository": {
    "type": "git",
    "url": "git+https://github.com/pocka/yup-locale-ja.git"
  },
  "keywords": [
    "yup",
    "locale"
  ],
  "main": "cjs/index.js",
  "module": "esm/index.js",
  "sideEffects": false,
  "files": [
    "esm",
    "cjs",
    "es5",
    "README_ja.md"
  ],
  "peerDependencies": {
    "yup": "0.x"
  },
  "devDependencies": {
    "@types/yup": "^0.29.3",
    "husky": "^4.2.5",
    "lint-staged": "^10.2.11",
    "prettier": "^2.0.5",
    "typescript": "^5.0.2",
    "vite": "^4.2.1",
    "vitest": "^0.29.7",
    "yup": "^0.29.2"
  },
  "scripts": {
    "build:esm": "tsc --outDir esm",
    "build:cjs": "tsc --outDir cjs -m commonjs",
    "build:es5": "tsc --outDir es5 -t ES5",
    "build": "yarn build:esm && yarn build:cjs && yarn build:es5",
    "test": "vitest",
    "prepack": "yarn build"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.{js,ts,md,json}": "prettier --write"
  },
  "prettier": {},
  "packageManager": "yarn@3.5.0"
}
